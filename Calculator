

def calc():
    num_one = int(input('Enter Integer or Float'))
    num_two = int(input('Enter Integer or Float'))
    calculate = str(input('Enter [ - + / * ]'))
    if calculate == '/':
        print(num_one / num_two)
    if calculate == '+':
        print(num_one + num_two)
    if calculate == '-':
        print(num_one - num_two)
    if calculate == '+':
        print(num_one + num_two)
    if calculate == "*":
        print(num_one * num_two)

calc()


